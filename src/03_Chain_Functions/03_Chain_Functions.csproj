<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <RootNamespace>_03_Chain_Functions</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Plugins\RoleTalk\Policeman\config.json" />
    <None Remove="Plugins\RoleTalk\Policeman\skprompt.txt" />
    <None Remove="Plugins\RoleTalk\Scientist\config.json" />
